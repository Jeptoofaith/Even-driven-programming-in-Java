    }                                    

    private void exitActionPerformed(java.awt.event.ActionEvent evt) {                                     
        // TODO add your handling code here:
  {
		if (e.getSource() == exit) {
			String def = "";
                        tid.setText(def);
			tname.setText(def);
			taddress.setText(def);
			tcontact.setText(def);
			tout.setText(def);
		}
    }                                    

    private void tidActionPerformed(java.awt.event.ActionEvent evt) {                                    
        // TODO add your handling code here:
    }                                   

    private void registerActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        if (e.getSource() == register) { 
			{       String data1;
				String data
					= "ID : "
					+ tid.getText() + "\n";
                                        data1 = "Name : "
					+ tname.getText() + "\n";
				if (male.isSelected())
					data1 = "Gender : Male"
							+ "\n";
				else
					data1 = "Gender : Female"
							+ "\n";
				String data2 = "Address : " + taddress.getText();
                                String data3 = "Contact : " + tcontact.getText();
               
		}
	}
    }                                        

    private void toutAncestorAdded(javax.swing.event.AncestorEvent evt) {                                   
        // TODO add your handling code here:
        
        tout.setText(data + data1 + data2 + data3);
				tout.setEditable(false);
    }                                  

